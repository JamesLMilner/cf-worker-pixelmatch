{
  "name": "my-app",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rm -rf ./transpiled/src ./transpiled/test",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test"
  },
  "author": "James <jamesmilner22@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^1.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "assemblyscript": "git@github.com:AssemblyScript/assemblyscript.git",
    "chai": "^4.2.0",
    "file-loader": "^6.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "service-worker-mock": "^2.0.3",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.12",
    "ts-loader": "^9.4.2",
    "typescript": "^3.5.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@types/pngjs": "^3.3.2",
    "base64-arraybuffer": "^0.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "pngjs": "^3.4.0",
    "sinon": "^7.4.2",
    "string-to-stream": "^3.0.1"
  }
}
